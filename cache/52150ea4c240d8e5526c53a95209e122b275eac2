                $(".c-filter a.active").removeClass("active");
                $(this).addClass("active");
                
                $(".categories-list li.secondary").addClass("hidden");
                return false;
            })

        
jQuery('#Filter_geo').select2({'formatNoMatches':function(){return "Совпадений не найдено";},'formatInputTooShort':function(input,min){return "Начните вводить название города";},'formatInputTooLong':function(input,max){return "Please enter "+(input.length-max)+" less characters";},'formatSelectionTooBig':function(limit){return "Вы можете выбрать только "+limit+" значений";},'formatLoadMore':function(pageNumber){return "Показать больше...";},'formatSearching':function(){return "Поиск...";},'placeholder':'Город','minimumInputLength':0,'ajax':{'url':'/city/ajaxData','quietMillis':0,'dataType':'json','data': function(term, page) {
                                                return {
                                                    q: term,
                                                    page_limit: 10,
                                                };
                                            },'results': function(data, page){
                                                return {results: data};
                                           }},'formatResult':function(data){
                                            if(data.level>2 && data.region_title) return data.title + " ("+data.region_title+")";
                                            else return data.title;
                                        },'formatSelection': function(data) {
                                             if(data.level>2 && data.region_title) return data.title + " ("+data.region_title+")";
                                            else return data.title;
                                        },'initSelection': function(element, callback){
                                            var id = $(element).val();
                                            if (id !== "") {
                                                $.ajax("/city/ajaxDataSelected?id=" + id, {
                                                    dataType: "json"
                                                }).done(function(data) { callback(data); });
                                            }
                                       }});
jQuery('#Filter_search_string').autocomplete({'minLength':'3','position':{'my':'right-9 top+9','at':'right bottom'},'select':function(event, ui){
                                                $("#Filter_search_string").val(ui.item.value);
                                                $("#header .default-form").submit();
                                            },'source':'/site/searchAuto'});
});
