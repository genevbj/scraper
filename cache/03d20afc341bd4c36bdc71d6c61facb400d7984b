<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {
    
            $('#changeRegion select').selectmenu({
                change:function(event,ui) {
                    window.location = $(this).val();
                }
            });
            $('#city-search').on('submit', function(){
                var url = $(this).attr('action');
                $.ajax({
                    url:      url,
                    type:     'POST',
                    dataType: 'html',
                    data: $(this).serialize(),
                    success: function(response) {
                        $('.search-container').html(response)
                        if ($('.search-container .town-list li').length==1) {
                            var go = $('.search-container .town-list li a').attr('href');
                            window.location = go;
                        }
                    },
                    error: function(response) {
                        //alert('Error');
                    }
                });
                return false;
            });

            $('#city-search input[name=title]').on('keyup', function(e){
                var form = $(this).closest('form');
                var url = form.attr('action');
                var val = $(this).val();
                if (val.length > 3) {
                    $.ajax({
                        url:      url,
                        type:     'POST',
                        dataType: 'html',
                        data: form.serialize(),
                        success: function(response) {
                            $('.search-container').html(response)
                        },
                        error: function(response) {
                            //alert('Error');
                        }
                    });
                }
                return false;
            });
        
});
/*]]>*/
</script>
