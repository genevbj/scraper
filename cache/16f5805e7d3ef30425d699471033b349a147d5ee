                                            },'results': function(data, page){
                                                return {results: data};
                                           }},'formatResult':function(data){
                                            if(data.level>2 && data.region_title) return data.title + " ("+data.region_title+")";
                                            else return data.title;
                                        },'formatSelection': function(data) {
                                             if(data.level>2 && data.region_title) return data.title + " ("+data.region_title+")";
                                            else return data.title;
                                        },'initSelection': function(element, callback){
                                            var id = $(element).val();
                                            if (id !== "") {
                                                $.ajax("/city/ajaxDataSelected?id=" + id, {
                                                    dataType: "json"
                                                }).done(function(data) { callback(data); });
                                            }
                                       }});
jQuery('#Filter_search_string').autocomplete({'minLength':'3','position':{'my':'right-9 top+9','at':'right bottom'},'select':function(event, ui){
                                                $("#Filter_search_string").val(ui.item.value);
                                                $("#header .default-form").submit();
                                            },'source':'/site/searchAuto'});
});
/*]]>*/
</script>
</body>
</html>